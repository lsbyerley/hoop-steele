{
  "name": "steele-hoops",
  "version": "2.0.0",
  "description": "Uses stats gathered from the web to predict college basketball games",
  "main": "main.js",
  "scripts": {
    "client:dev": "parcel index.html",
    "server:dev": "nodemon ./server/app.js --watch ./server",
    "dev": "concurrently --kill-others 'npm run client:dev' 'npm run server:dev' || true",
    "client:prod": "parcel build index.html",
    "server:prod": "node ./server/app.js",
    "build": "npm run client:prod & npm run server:prod || true",
    "prebuild": "rm -rf dist"
  },
  "author": "lsbyerley@gmail.com",
  "license": "ISC",
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dayjs": "^1.7.7",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "memory-cache": "^0.2.0",
    "mustard-ui": "0.0.8",
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@vue/component-compiler-utils": "^2.3.0",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.7",
    "parcel-bundler": "^1.10.3",
    "sass": "^1.14.3",
    "vue-hot-reload-api": "^2.3.1",
    "vue-template-compiler": "^2.5.17"
  }
}
